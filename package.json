{
  "name": "nextjs-web-app",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "dev": "next dev -p 3000",
    "build": "next build",
    "start": "next start",
    "lint": "next lint",
    "test": "jest",
    "prisma-studio": "dotenv -e .env.local prisma studio",
    "prisma-introspect_db": "dotenv -e .env.local prisma db pull",
    "prisma-deploy-schema": "prisma migrate diff --from-empty --to-url=postgresql://postgres:admin@localhost:5432/OT?schema=public --script > prisma/migrations/0_init/migration.sql",
    "prisma-generate-client-instance": "dotenv -e .env.local prisma generate",
    "format": "prettier --write \"**/*.{js,jsx,ts,tsx,json,md}\""
  },
  "jest": {
    "verbose": true,
    "collectCoverage": true,
    "preset": "ts-jest"
  },
  "dependencies": {
    "@emotion/react": "^11.11.0",
    "@emotion/styled": "^11.11.0",
    "@floating-ui/react": "^0.24.1",
    "@mui/material": "^5.13.1",
    "@prisma/client": "^4.14.1",
    "@react-three/drei": "^9.68.3",
    "@react-three/fiber": "^8.13.0",
    "@reduxjs/toolkit": "^1.9.5",
    "@types/three": "^0.152.0",
    "d3": "^7.8.4",
    "next": "13.4.2",
    "pg": "^8.11.0",
    "react": "18.2.0",
    "react-dom": "18.2.0",
    "react-redux": "^8.0.5",
    "reflect-metadata": "^0.1.13",
    "server-only": "^0.0.1",
    "three": "^0.152.2"
  },
  "devDependencies": {
    "@types/d3": "^7.4.0",
    "@types/jest": "^29.5.1",
    "@types/node": "^20.1.4",
    "@types/react": "18.2.6",
    "@types/react-dom": "18.2.4",
    "@typescript-eslint/eslint-plugin": "^5.59.5",
    "@typescript-eslint/parser": "^5.59.5",
    "autoprefixer": "10.4.14",
    "eslint": "8.40.0",
    "eslint-config-next": "13.4.2",
    "eslint-config-prettier": "^8.8.0",
    "eslint-plugin-markdown": "^3.0.0",
    "eslint-plugin-prettier": "^4.2.1",
    "eslint-plugin-yaml": "^0.5.0",
    "jest": "^29.5.0",
    "postcss": "8.4.23",
    "prettier": "2.8.8",
    "prisma": "^4.14.1",
    "tailwindcss": "3.3.2",
    "typescript": "5.0.4"
  },
  "engines": {
    "npm": ">=8.0.0",
    "node": ">=16.0.0"
  }
}
